{
  "name": "bot-be",
  "private": true,
  "version": "1.5.0",
  "description": "Bot data service",
  "license": "MIT",
  "scripts": {
    "copy-files": "copyfiles -u 1 src/templates/* dist/",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "npm run copy-files",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --fix \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-esm -d src/migration-data-source.ts",
    "migration:generate": "npm run typeorm migration:generate",
    "migration:create": "typeorm migration:create",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.312.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.5",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.5",
    "@nestjs/platform-express": "^9.0.5",
    "@nestjs/swagger": "^6.0.4",
    "aws-sdk": "^2.1182.0",
    "axios": "^1.2.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "csv-parser": "^3.0.0",
    "date-fns": "^2.29.2",
    "date-fns-tz": "^1.3.7",
    "dotenv": "^16.0.1",
    "express-basic-auth": "^1.2.1",
    "express-handlebars": "^5.3.5",
    "fast-csv": "^4.3.6",
    "jwks-rsa": "^2.1.4",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.7.3",
    "pg-query-stream": "^4.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "stripe": "^11.6.0",
    "swagger-stats": "^0.99.5",
    "swagger-ui-express": "^4.5.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.5",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.6.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-boundaries": "^2.10.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-loader": "9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "pre-commit": [
//    "lint",
    "build"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
